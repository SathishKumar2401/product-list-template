{
  "mobileJson": {
    "canvasWidth": 310,
    "imageHeight": "116px",
    "renderImage": "./assets/tools/home_sections.png",
    "verticalPosition": "450px",
    "horizontalPosition": "240px"
},
  "desktopJson": {
    "domTree": {
      "tag": "div",
      "style": {
        "gap": "10px",
        "width": "100%",
        "height": "150vh",
        "display": "flex",
        "position": "relative",
        "justifyContent": "space-between"
      },
      "scripts": {
        "handleClick": {
          "body": "const event = new CustomEvent('productListHandleClick', {detail:value});document.dispatchEvent(event);",
          "params": [
            "value"
          ]
        }
      },
      "cssStyles": {
        ".zwe-product-card:hover": {
          "border": "1px solid black !important"
        },
        ".zwe-product-card:hover .quick-view-parent": {
          "opacity": "1 !important",
          "visibility": "visible !important"
        },
        ".zwe-product-card:hover .add-to-bag-parent": {
          "opacity": "1 !important",
          "visibility": "visible !important"
        },
        ".product-list-img-container:hover ~ .productquickView, .productquickView:hover": {
          "opacity": "1 !important",
          "transform": "translateY(0)",
          "zIndex": "1 !important",
          "cursor": "pointer"
        }
      },
      "children": [
        {
          "tag": "zwe-product-filter",
          "style": {
            "top": "0",
            "left": "0",
            "width": "25%",
            "height": "100%"
          },
          "attributes": {
            "id": "mbrtfype_1nqkn",
            "data-id": "webComponent"
          },
          "configuration": {
            "filterData": {
              "sortArray": [
                {
                  "value": {
                    "order": "ASC",
                    "fieldName": "name"
                  },
                  "display": "Name : Ascending",
                  "isVisible": true
                },
                {
                  "value": {
                    "order": "DESC",
                    "fieldName": "name"
                  },
                  "display": "Name : Descending",
                  "isVisible": true
                },
                {
                  "value": {
                    "order": "DESC",
                    "fieldName": "price"
                  },
                  "display": "Price : Highest first",
                  "isVisible": true
                },
                {
                  "value": {
                    "order": "ASC",
                    "fieldName": "price"
                  },
                  "display": "Price : Lowest first",
                  "isVisible": true
                }
              ],
              "filterArray": [
                {
                  "type": "check-box",
                  "options": [
                    {
                      "value": {
                        "end": 499,
                        "begin": 0
                      },
                      "display": "$0 - $499"
                    },
                    {
                      "value": {
                        "end": 999,
                        "begin": 500
                      },
                      "display": "$500 - $999"
                    },
                    {
                      "value": {
                        "end": 1999,
                        "begin": 1000
                      },
                      "display": "$1000 - $1999"
                    },
                    {
                      "value": {
                        "end": 3999,
                        "begin": 2000
                      },
                      "display": "$2000 - $3999"
                    },
                    {
                      "value": {
                        "end": 10000,
                        "begin": 4000
                      },
                      "display": "$4000 - $10000"
                    }
                  ],
                  "fieldName": "price",
                  "isDefault": true,
                  "isVisible": true,
                  "filterName": "Price",
                  "optionType": "Range"
                },
                {
                  "type": "radio-option",
                  "options": [
                    {
                      "value": {
                        "op": ">",
                        "value": 4
                      },
                      "display": "4 stars & above"
                    },
                    {
                      "value": {
                        "op": ">",
                        "value": 3
                      },
                      "display": "3 stars & above"
                    },
                    {
                      "value": {
                        "op": ">",
                        "value": 2
                      },
                      "display": "2 stars & above"
                    },
                    {
                      "value": {
                        "op": ">",
                        "value": 1
                      },
                      "display": "1 star & above"
                    }
                  ],
                  "fieldName": "rating",
                  "isDefault": true,
                  "isVisible": true,
                  "filterName": "Customer Ratings",
                  "optionType": "Condition"
                },
                {
                  "type": "radio-option",
                  "options": [
                    {
                      "value": "inStock",
                      "display": "Instock"
                    },
                    {
                      "value": "outOfStock",
                      "display": "Out of stock"
                    }
                  ],
                  "fieldName": "stock",
                  "isDefault": true,
                  "isVisible": true,
                  "filterName": "Stock",
                  "optionType": "Simple"
                }
              ]
            },
            "filterMode": "normal",
            "filterTemplate": "filterTemplateOne",
            "showFoodFilter": false,
            "showSortOption": false,
            "showFilterButton": false,
            "showFilterSection": true,
            "filterTemplateTextColor": "",
            "filterTemplateBackgroundColor": ""
          }
        },
        {
          "tag": "zb-product-list",
          "card": {
            "tag": "div",
            "style": {
              "width": "232px",
              "height": "320px",
              "border": "1px solid #eee",
              "position": "relative"
            },
            "events": {
              "click": {
                "function": "handleClick",
                "params": [
                  "navigateTo"
                ]
              }
            },
            "children": [
              {
                "tag": "div",
                "style": {
                  "maxWidth": "230px",
                  "height": "230px",
                  "position": "absolute",
                  "overflow": "hidden",
                  "cursor": "pointer",
                  "data-id": "productImage"
                },
                "children": [
                  {
                    "tag": "img",
                    "style": {
                      "width": "100%",
                      "height": "100%",
                      "position": "relative",
                      "objectFit": "contain"
                    },
                    "attributes": {
                      "id": "mbrtfype_6dkbn",
                      "alt": "${name}",
                      "src": "${ featuredImage ? awsUrl+featuredImage : 'assets/image-not-available.png'}",
                      "type": "image",
                      "class": "productImage",
                      "isEnabled": true,
                      "zb-other-angle": true
                    }
                  }
                ],
                "attributes": {
                  "isEnabled": true,
                  "class": "product-list-img-container",
                  "data-id": "productImage"
                }
              },
              {
                "tag": "a",
                "style": {
                  "position": "absolute",
                  "padding": "5px 12px",
                  "height": "50px",
                  "top": "235px",
                  "width": "207px",
                  "left": "11px",
                  "fontSize": "var(--product-list-font-size,15px)",
                  "fontWeight": "700",
                  "textAlign": "center",
                  "color": "var(--dark-color,black)",
                  "lineHeight": "1.5",
                  "overflow": "hidden",
                  "cursor": "pointer",
                  "textOverflow": "ellipsis",
                  "display": "-webkit-box",
                  "wordBreak": "break-word",
                  "WebkitLineClamp": "2",
                  "WebkitBoxOrient": "vertical"
                },
                "attributes": {
                  "id": "mbrtfype_b2lb0",
                  "type": "text",
                  "class": "productTitle ",
                  "isEnabled": true,
                  "title": "${name}",
                  "data-id": "productName"
                },
                "textContent": "${name}"
              },
              {
                "tag": "span",
                "style": {
                  "position": "absolute",
                  "top": "290px",
                  "height": "20px",
                  "width": "70px",
                  "textAlign": "center",
                  "padding": "5px 12px",
                  "fontSize": "13px",
                  "color": "black",
                  "fontWeight": "700"
                },
                "attributes": {
                  "id": "mbrtfype_pxcpv",
                  "type": "text",
                  "class": "productPrice ",
                  "isEnabled": true,
                  "data-id": "productPrice"
                },
                "textContent": "${currency}${price}"
              },
              {
                "tag": "span",
                "condition": "${weightUnit}",
                "style": {
                  "position": "absolute",
                  "top": "289px",
                  "left": "60px",
                  "width": "70px",
                  "height": "20px",
                  "padding": "5px 12px",
                  "fontSize": "15px",
                  "color": "grey",
                  "fontWeight": "400"
                },
                "attributes": {
                  "id": "mbrtfype_pxcpv",
                  "type": "text",
                  "class": "weight-unit ",
                  "isEnabled": true,
                  "data-id": "productWeightUnit"
                },
                "textContent": "/ ${weightUnit}"
              },
              {
                "tag": "span",
                "style": {
                  "top": "294px",
                  "left": "130px",
                  "width": "100px",
                  "height": "20px",
                  "position": "absolute",
                  "fontWeight": "400",
                  "textDecoration": "line-through",
                  "color": "grey",
                  "alignItems": "center",
                  "marginTop": "2px",
                  "fontSize": "calc(var(--product-list-font-size,15px) - 2px)",
                  "display": "${costComparePrice ? 'inline':'none'}"
                },
                "attributes": {
                  "id": "mbrtfype_lcuai",
                  "type": "text",
                  "class": "productCostComparePrice ",
                  "isEnabled": true,
                  "data-id": "productCostComparePrice"
                },
                "condition": "${costComparePrice}",
                "textContent": "${currency}${costComparePrice}"
              },
              {
                "tag": "span",
                "style": {
                  "position": "absolute",
                  "fontSize": "13px",
                  "left": "5px",
                  "bottom": "119px",
                  "fontWeight": "700",
                  "padding": "7px",
                  "color": "white",
                  "borderRadius": "16px",
                  "border": "1px solid var(--element-primary-color,grey)",
                  "backgroundColor": "var(--element-primary-color,grey)"
                },
                "attributes": {
                  "id": "mbrtfype_77pkg",
                  "type": "text",
                  "class": "discountPercentage",
                  "isEnabled": true,
                  "data-id": "productDiscount"
                },
                "textContent": " % OFF"
              },
              {
                "tag": "span",
                "style": {
                  "top": "308px",
                  "left": "7px",
                  "width": "280px",
                  "height": "45px",
                  "position": "absolute",
                  "fontSize": "16px",
                  "color": "#000000",
                  "fontWeight": "400",
                  "display": "none"
                },
                "attributes": {
                  "id": "mbrtfype_9jiii",
                  "type": "text",
                  "class": "productDescription ",
                  "isEnabled": false,
                  "data-id": "productDescription"
                },
                "condition": "${shortDescription}",
                "textContent": "${shortDescription}"
              },
              {
                "tag": "div",
                "condition": "${(multiLocationVariantStocks[0]?.stock >= 1) && !(multiLocationVariantStocks[0]?.isPreorder) && !(isDigitalProduct)}",
                "attributes": {
                  "class": "add-to-bag-parent",
                  "title": "Add to bag",
                  "data-id": "productCartButton"
                },
                "events": {
                  "click": {
                    "function": "handleClick",
                    "params": [
                      "navigateTo"
                    ]
                  }
                },
                "style": {
                  "backgroundColor": "var(--element-primary-color,grey)",
                  "width": "35px",
                  "height": "35px",
                  "top": "20px",
                  "left": "190px",
                  "position": "absolute",
                  "borderRadius": "50%",
                  "color": "white",
                  "opacity": "0",
                  "visibility": "hidden",
                  "transition": "opacity 0.5s ease, visibility 0.5s ease"
                },
                "children": [
                  {
                    "tag": "span",
                    "attributes": {
                      "data-id": "opaline-tiles-video-control-section",
                      "class": "material-symbols-outlined fill add-to-bag-icon",
                      "type": "icon"
                    },
                    "style": {
                      "position": "relative",
                      "top": "-1px",
                      "left": "-1px",
                      "zIndex": "1",
                      "textAlign": "center",
                      "fontSize": "17px",
                      "borderRadius": "50%",
                      "cursor": "pointer",
                      "padding": "10px"
                    },
                    "textContent": "enhanced_encryption"
                  }
                ]
              },
              {
                "tag": "div",
                "condition": "${!isDigitalProduct}",
                "attributes": {
                  "class": "quick-view-parent",
                  "title": "Quick view",
                  "data-id": "productQuickView"
                },
                "events": {
                  "click": {
                    "function": "handleClick",
                    "params": [
                      "quickView"
                    ]
                  }
                },
                "style": {
                  "backgroundColor": "var(--element-primary-color, grey)",
                  "width": "35px",
                  "height": "35px",
                  "top": "60px",
                  "left": "190px",
                  "position": "absolute",
                  "opacity": "0",
                  "visibility": "hidden",
                  "transition": "opacity 0.5s ease, visibility 0.5s ease",
                  "borderRadius": "50%"
                },
                "children": [
                  {
                    "tag": "span",
                    "attributes": {
                      "data-id": "opaline-tiles-video-control-section",
                      "class": "material-symbols-outlined fill quick-view-icon",
                      "type": "icon"
                    },
                    "style": {
                      "position": "relative",
                      "top": "-1px",
                      "left": "-1px",
                      "zIndex": "1",
                      "textAlign": "center",
                      "fontSize": "17px",
                      "color": "white",
                      "borderRadius": "50%",
                      "cursor": "pointer",
                      "padding": "10px"
                    },
                    "textContent": "visibility"
                  }
                ]
              }
            ],
            "attributes": {
              "id": "mbrtfype_jyd2w",
              "class": "zwe-product-card container",
              "productIndex": "${index}"
            }
          },
          "style": {
            "top": "0",
            "right": "0",
            "width": "75%",
            "height": "100%",
            "overflow": "hidden scroll"
          },
          "pageName": "Product list",
          "attributes": {
            "id": "mbrtfype_s4yhm",
            "data-id": "webComponent",
            "discountFormat": "{discountPercentage}% OFF"
          },
          "elementsList": [
            {
              "id": "product-list-container",
              "tag": "box",
              "elementName": "Box",
              "isWebElement": true,
              "updatedJsonName": "boxStyles"
            }
          ],
          "configuration": {
            "api": "featuredProducts",
            "datas": [
              "name",
              "price",
              "featuredImage",
              "costComparePrice"
            ],
            "productCardDesign": "chocolateProductList",
            "columnGap": "",
            "backgroundColor": ""
          },
          "isConfigDialog": true
        }
      ],
      "attributes": {
        "id": "mbrtfype_2cidh"
      }
    },
    "canvasWidth": 1351,
    "imageHeight": "116px",
    "renderImage": "./assets/tools/home_sections.png",
    "verticalPosition": "450px",
    "horizontalPosition": "240px"
  },
  "sectionCode": "PRODUCT_LIST",
  "sectionName": "Product List"
}