          "color": "black !important",
          "border": "1px dotted var(--element-primary-color,grey) !important"
        },
        ".add-to-cart-icon:hover, .zwe-product-card container:has(.zwe-pl-add-to-cart-btn:hover) .add-to-cart-icon": {
          "color": "black !important"
        }
