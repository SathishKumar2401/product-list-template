          "configuration": {
            "api": "featuredProducts",
            "datas": [
              "name",
              "price",
              "featuredImage",
              "costComparePrice"
            ],
            "productCardDesign": "groceryFriendlyDesign",
            "columnGap": "",
            "backgroundColor": ""
          },
          "isConfigDialog": true
        }
