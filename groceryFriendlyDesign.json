                },
                {
                  "value": {
                    "order": "DESC",
                    "fieldName": "name"
                  },
                  "display": "Name : Descending",
                  "isVisible": true
                },
                {
                  "value": {
                    "order": "DESC",
                    "fieldName": "price"
                  },
                  "display": "Price : Highest first",
                  "isVisible": true
                },
                {
                  "value": {
                    "order": "ASC",
                    "fieldName": "price"
                  },
                  "display": "Price : Lowest first",
                  "isVisible": true
                }
              ],
              "filterArray": [
                {
                  "type": "check-box",
                  "options": [
                    {
                      "value": {
                        "end": 499,
                        "begin": 0
